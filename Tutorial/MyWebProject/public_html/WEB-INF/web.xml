<?xml version = '1.0' encoding = 'EUC-KR'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <module-name>MyWebProject</module-name>
    <display-name>MyWebProject</display-name>
    <description>Learning servlets</description>
    <servlet>
        <servlet-name>HelloWorld</servlet-name>
        <servlet-class>com.project.tutorial.webproject.HelloWorld</servlet-class>
        <init-param>
            <param-name>MyText</param-name>
            <param-value>HEllo this is my text</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>HelloWorld</servlet-name>
        <url-pattern>/HelloWorld</url-pattern>
    </servlet-mapping>
</web-app>
